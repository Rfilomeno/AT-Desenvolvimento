<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.filomeno.rm.at_desenvolvimento_rodrigo_filomeno">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CONTROLLER.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CONTROLLER.CadastroActivity"
            android:label="Cadastro"
            android:parentActivityName=".CONTROLLER.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.filomeno.rm.at_desenvolvimento_rodrigo_filomeno.CONTROLLER.MainActivity" />
            >
        </activity>
        <activity android:name=".CONTROLLER.TarefasActivity"
            android:label="Tarefas"
            android:parentActivityName=".CONTROLLER.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.filomeno.rm.at_desenvolvimento_rodrigo_filomeno.CONTROLLER.MainActivity" />
            >></activity>
    </application>

</manifest>